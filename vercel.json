{
  "headers": [
    {
      "source": "/docs/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/preview/CHANGELOG.md",
      "destination": "/CHANGELOG.md"
    },
    {
      "source": "/preview/:path*",
      "destination": "/docs/:path*"
    }
  ]
}