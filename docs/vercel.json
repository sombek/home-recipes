{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/docs"
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/docs/:path*"
    }
  ]
}